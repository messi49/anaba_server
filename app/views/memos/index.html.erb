<h1>Listing memos</h1>

<table>
  <thead>
    <tr>
      <th>User_id</th>
      <th>Venue</th>
      <th>Venue_id</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user.each do |one| %>
      <tr>
        <td><%= one.user_id %></td>
        <td><%= one.venue %></td>
        <td><%= one.venue_id %></td>
        <td><%= link_to 'Show', one %></td>
        <td><%= link_to 'Edit', edit_memo_path(one) %></td>
        <td><%= link_to 'Destroy', one, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Memo', new_memo_path %>
